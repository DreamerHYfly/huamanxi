/* pages/search/search.wxss */

page {
  background-color: #d3cdb7;
}

.weui-search-bar {
  position: relative;
  padding: 8px 10px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  box-sizing: border-box;
  background-color: #d3cdb7;
  /* border-top: 1rpx solid #D7D6DC;
  border-bottom: 1rpx solid #D7D6DC; */
}

.weui-icon-search {
  margin-right: 4px;
  font-size: inherit;
}

.weui-icon-search_in-box {
  position: absolute;
  left: 10px;
  top: 7px;
}

.weui-search-bar__text {
  display: inline-block;
  font-size: 16px;
}

.weui-search-bar__form {
  position: relative;
  -webkit-box-flex: 1;
  -webkit-flex: auto;
  flex: auto;
  border-radius: 43px;
  background: #fff;
  border: 1rpx solid #e6e6ea;
}

.weui-search-bar__box {
  position: relative;
  padding-left: 30px;
  padding-right: 30px;
  width: 100%;
  box-sizing: border-box;
  z-index: 1;
}

.weui-search-bar__input {
  height: 28px;
  line-height: 28px;
  font-size: 14px;
}

.weui-search-bar__label {
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  z-index: 2;
  border-radius: 43px;
  text-align: center;
  color: #9b9b9b;
  background: #fff;
  line-height: 28px;
}

.weui-search-bar__cancel-btn {
  margin-left: 10px;
  line-height: 28px;
  color: #2c3f46;
  white-space: nowrap;
}

.weui-icon-clear {
  position: absolute;
  right: 10px;
  top: 3px;
}

.h1 {
  font-size: 100%;
  padding: 0px;
  margin-top: 0px;
  margin-right: 0px;
  margin-bottom: 0px;
  margin-left: 0px;
  color: #0f0f0f;
  display: block;
  margin-block-start: 0.67em;
  margin-block-end: 0.67em;
  margin-inline-start: 0px;
  margin-inline-end: 0px;
  font-weight: bold;
  line-height: 40px;
}

.titletype {
  width: 90%;
  height: auto;
  margin: 0 auto;
  margin-top: 20px;
  background-color: #f0efe2;
  overflow: hidden;
  border: 1px solid #c8c7bc;
  -moz-border-radius: 5px;
  -webkit-border-radius: 5px;
  border-radius: 5px;
}

.titletype .son1 {
  clear: both;
  height: 40px;
  line-height: 40px;
  margin-top: 5px;
  font-size: 20px;
  border-bottom: 1px solid #dad9d1;
  margin-left: 20px;
  margin-right: 20px;
}

.titletype .son1 span {
  float: right;
  font-size: 12px;
  height: 40px;
  line-height: 40px;
  /* margin-top: 5px; */
  font-weight: normal;
  color: #999;
}

.titletype .son2 {
  width: 90%;
  margin-left: 20px;
  clear: both;
  border-bottom: 1px dashed #dad9d1;
  overflow: hidden;
  padding-bottom: 10px;
}
.titletype .son2:last-child {
  border-bottom: 0 dashed #dad9d1;
}

.titletype .sleft {
  width: 50px;
  float: left;
  color: #275f38;
}

.titletype .sleft span {
  float: left;
  margin-top: 14px;
  font-size: 14px;
}

.titletype .sleft a {
  float: left;
  margin-top: 14px;
  font-size: 14px;
}

.titletype .sright {
  width: 74%;
  float: left;
}

.titletype .sright a {
  float: left;
  margin-top: 14px;
  width: 25%;
  font-size: 14px;
}

.titletype .sright span {
  float: left;
  margin-top: 14px;
  width: 50px;
  color: #b00815;
  font-size: 14px;
}

.titletype .sright2 {
  width: 15px;
  float: right;
  margin-top: 14px;
  
  z-index: 33;
}

.titletype .sright2 .img {
  cursor: pointer;
  width: 10%;
  height: 10%;
}


.rotate{
  transform: rotate(180deg);
}