/**app.wxss**/
@import '/style/weui.wxss';


.weui-cells{
  background-color: #d3cdb7;
}
.weui-cells__title {
  text-align: center;
  margin-top: 50rpx;
}

page {
  background-color: #d3cdb7;
}

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

.nav {
  padding: 5rpx 60rpx;
  border-bottom: 1rpx solid rgb(102, 102, 100);
  border-top: 1rpx solid rgb(194, 199, 129);
  position: relative;
}

.nav image {
  width: 40px;
  height: 40px;
  padding-top: 4px;
}

.nav .search {
  float: right;
  padding-top: 4px;
}

.bg-img {
  width: 100%;
  height: 100%;
  position: fixed;
  z-index: -1;
}
